{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "gen": {
      "outputs": ["__generated__/**"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["gen"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "gen"],
      "outputs": ["dist/**"]
    }
  }
}
