{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["validate-env"],
      "cache": false
    },
    "gen": {
      "outputs": ["__generated__/**"],
      "cache": false
    },
    "validate-env": {
      "cache": false
    }
  }
}
