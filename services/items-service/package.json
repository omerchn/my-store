{
  "name": "items-service",
  "version": "0.0.0",
  "private": true,
  "main": "proto/export.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child ./src/server-ts-proto.ts",
    "dev:mali": "ts-node-dev --respawn --transpile-only --exit-child ./src/server-mali.ts",
    "gen:service": "sh ./proto/generate-ts.sh",
    "validate-env": "ts-node src/env.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@stargate-oss/stargate-grpc-node-client": "^0.2.3",
    "dotenv": "^16.0.3",
    "mali": "^0.47.1",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "grpc-tools": "^1.11.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^1.132.1",
    "tsconfig": "*",
    "typescript": "^4.9.3"
  }
}
